{% extends 'base.html.twig' %}

{% block title %}Service{% endblock %}

{% block body %}
    <h1>Service</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ service.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ service.name }}</td>
            </tr>
            <tr>
                <th>Info</th>
                <td>{{ service.info }}</td>
            </tr>
            <tr>
                <th>Capacity</th>
                <td>{{ service.capacity }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ service.email }}</td>
            </tr>
            <tr>
                <th>PhoneNumber</th>
                <td>{{ service.phoneNumber }}</td>
            </tr>
            <tr>
                <th>Street</th>
                <td>{{ service.street }}</td>
            </tr>
            <tr>
                <th>Zip</th>
                <td>{{ service.zip }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ service.city }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ service.url }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ service.type }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('service_index') }}">back to list</a>

    <a href="{{ path('service_edit', {'id': service.id}) }}">edit</a>

    {{ include('service/_delete_form.html.twig') }}
{% endblock %}
